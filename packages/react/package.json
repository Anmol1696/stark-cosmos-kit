{
  "name": "@cosmos-kit/react",
  "version": "2.0.0",
  "description": "cosmos-kit wallet connector",
  "author": "developers@cosmology.tech",
  "contributors": [
    {
      "name": "Jun Liu"
    },
    {
      "name": "Dan Lynch"
    },
    {
      "name": "Noah Saso"
    },
    {
      "name": "Eliot Baker"
    },
    {
      "name": "Delivan Jeonghyeok Yoo"
    }
  ],
  "homepage": "https://github.com/cosmology-tech/cosmos-kit#readme",
  "license": "SEE LICENSE IN LICENSE",
  "source": "./src/index.ts",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "tsup": {
    "clean": true,
    "dts": true,
    "dts-resolve": true,
    "entry": [
      "src/**/*.{ts,tsx}"
    ],
    "format": [
      "cjs",
      "esm"
    ],
    "splitting": false,
    "sourcemap": true,
    "bundle": true,
    "external": [
      "react"
    ]
  },
  "directories": {
    "lib": "src",
    "test": "__tests__"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "prepare": "npm rum build",
    "dev": "yarn watch",
    "watch": "tsup --watch",
    "lint": "eslint --ext .tsx,.ts .",
    "format": "eslint --ext .tsx,.ts --fix .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:debug": "node --inspect node_modules/.bin/jest --runInBand",
    "clear": "rm -rf ./dist"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cosmology-tech/cosmos-kit"
  },
  "keywords": [
    "cosmos",
    "cosmos-kit",
    "cosmostation",
    "leap",
    "keplr",
    "wallet"
  ],
  "bugs": {
    "url": "https://github.com/cosmology-tech/cosmos-kit/issues"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "dist/"
    ]
  },
  "devDependencies": {
    "@types/react": "^18.2",
    "@types/react-dom": "^18.2"
  },
  "dependencies": {
    "@chain-registry/types": "0.13.1",
    "@cosmology-ui/react": "1.1.0-beta.10",
    "@cosmos-kit/core": "^2.0.0",
    "@cosmos-kit/react-lite": "^2.0.0",
    "react-icons": "4.4.0"
  },
  "resolutions": {
    "@types/react": "18.2.0"
  },
  "peerDependencies": {
    "@cosmjs/cosmwasm-stargate": ">= 0.28",
    "@cosmjs/stargate": ">= 0.28",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "gitHead": "0f174bf8c1799158d87dfcfb97c642e51835b8d0"
}
