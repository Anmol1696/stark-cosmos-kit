{
  "name": "@cosmos-kit/react-lite",
  "version": "1.5.2-beta.6",
  "description": "cosmos-kit wallet connector",
  "author": "developers@cosmology.tech",
  "contributors": [
    {
      "name": "Jun Liu"
    },
    {
      "name": "Dan Lynch"
    },
    {
      "name": "Noah Saso"
    },
    {
      "name": "Eliot Baker"
    },
    {
      "name": "Delivan Jeonghyeok Yoo"
    }
  ],
  "homepage": "https://github.com/cosmology-tech/cosmos-kit#readme",
  "license": "SEE LICENSE IN LICENSE",
  "source": "./src/index.ts",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": {
        "import": "./dist/index.d.ts",
        "default": "./dist/index.d.ts"
      },
      "module": "./dist/index.mjs",
      "import": "./dist/index.mjs",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"},
  "tsup": {
    "clean": true,
    "dts": true,
    "dts-resolve": true,
    "entry": [
      "src/**/*.{ts,tsx}"
    ],
    "format": [
      "cjs",
      "esm"
    ],
    "splitting": false,
    "sourcemap": true
  },
  "directories": {
    "lib": "src",
    "test": "__tests__"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "prepare": "npm rum build",
    "dev": "yarn watch",
    "watch": "tsup --watch",
    "clear": "rm -rf ./dist",
    "lint": "eslint --ext .tsx,.ts .",
    "format": "eslint --ext .tsx,.ts --fix .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:debug": "node --inspect node_modules/.bin/jest --runInBand"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cosmology-tech/cosmos-kit"
  },
  "keywords": [
    "cosmos",
    "cosmos-kit",
    "cosmostation",
    "leap",
    "keplr",
    "wallet"
  ],
  "bugs": {
    "url": "https://github.com/cosmology-tech/cosmos-kit/issues"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "dist/"
    ]
  },
  "devDependencies": {
    "@babel/preset-react": "^7.22.5",
    "@cosmjs/cosmwasm-stargate": "0.29.0",
    "@cosmjs/stargate": "0.29.0",
    "@types/jest": "29.0.0",
    "@typescript-eslint/eslint-plugin": "5.38.0",
    "@typescript-eslint/parser": "5.38.0",
    "babel-jest": "29.0.3",
    "cross-env": "7.0.2",
    "eslint": "8.23.1",
    "eslint-config-next": "12.3.1",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-simple-import-sort": "8.0.0",
    "eslint-plugin-unused-imports": "2.0.0",
    "jest": "29.0.2",
    "jest-in-case": "1.0.2",
    "prettier": "2.1.2",
    "regenerator-runtime": "0.13.7",
    "ts-jest": "29.0.1",
    "tsup": "^7.1.0",
    "typescript": "4.8.3",
    "webpack": "5.74.0"
  },
  "dependencies": {
    "@chain-registry/types": "0.13.1",
    "@cosmos-kit/core": "^1.7.1-beta.6"
  },
  "resolutions": {
    "@types/react": "^18.2"
  },
  "peerDependencies": {
    "@cosmjs/cosmwasm-stargate": ">= 0.28",
    "@cosmjs/stargate": ">= 0.28",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "gitHead": "0f174bf8c1799158d87dfcfb97c642e51835b8d0"
}
