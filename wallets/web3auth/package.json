{
  "name": "@cosmos-kit/web3auth",
  "version": "2.0.3",
  "description": "cosmos-kit wallet connector for Web3Auth",
  "author": "noah@noah.zone",
  "contributors": [
    {
      "name": "Noah Saso"
    }
  ],
  "homepage": "https://github.com/cosmology-tech/cosmos-kit#readme",
  "license": "SEE LICENSE IN LICENSE",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "typings": "dist/index.d.ts",
  "directories": {
    "lib": "src",
    "test": "__tests__"
  },
  "files": [
    "dist",
    "!CHANGELOG.md"
  ],
  "scripts": {
    "build:mjs": "yarn tsc -p tsconfig.json --outDir dist --module es2022 --declaration false || true",
    "clean:dist": "rimraf dist",
    "clean": "npm run clean:dist",
    "build": "npm run clean && npm run build:mjs",
    "prepare": "npm run build",
    "lint": "eslint --ext .tsx,.ts .",
    "format": "eslint --ext .tsx,.ts --fix .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:debug": "node --inspect node_modules/.bin/jest --runInBand"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cosmology-tech/cosmos-kit"
  },
  "keywords": [
    "cosmos-kit",
    "cosmos",
    "leap",
    "wallet"
  ],
  "bugs": {
    "url": "https://github.com/cosmology-tech/cosmos-kit/issues"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "dist/"
    ]
  },
  "dependencies": {
    "@chain-registry/types": "0.13.1",
    "@cosmos-kit/core": "^2.0.3",
    "@solana/web3.js": "^1.77.3",
    "@toruslabs/eccrypto": "^2.1.1",
    "@web3auth/base": "^5.1.0",
    "@web3auth/base-provider": "^6.1.4",
    "@web3auth/ethereum-provider": "^6.1.5",
    "@web3auth/no-modal": "^6.1.4",
    "@web3auth/openlogin-adapter": "^6.1.4",
    "@web3auth/solana-provider": "^6.1.4",
    "ethereum-cryptography": "^2.1.2",
    "url": "^0.11.1"
  },
  "peerDependencies": {
    "@cosmjs/amino": ">= 0.28",
    "@cosmjs/proto-signing": ">= 0.28",
    "@keplr-wallet/cosmos": ">= 0.10",
    "cosmjs-types": ">= 0.5"
  },
  "gitHead": "2c20b02438f2715d62bc69df58380af9685c4b3d"
}
