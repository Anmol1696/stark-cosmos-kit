{
  "name": "@cosmos-kit/exodus",
  "version": "2.0.0",
  "description": "@cosmos-kit/exodus is the Exodus integration for CosmosKit",
  "author": "devs@exodus.io",
  "homepage": "https://github.com/cosmology-tech/cosmos-kit#readme",
  "license": "SEE LICENSE IN LICENSE",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "tsup": {
    "clean": true,
    "dts": true,
    "dts-resolve": true,
    "entry": [
      "src/**/*.ts"
    ],
    "format": [
      "cjs",
      "esm"
    ],
    "splitting": false,
    "sourcemap": true,
    "skipNodeModulesBundle": true
  },
  "directories": {
    "lib": "src"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "prepare": "npm rum build",
    "dev": "yarn watch",
    "watch": "tsup --watch",
    "lint": "eslint --ext .tsx,.ts .",
    "format": "eslint --ext .tsx,.ts --fix .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:debug": "node --inspect node_modules/.bin/jest --runInBand"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cosmology-tech/cosmos-kit"
  },
  "keywords": [
    "cosmos-kit",
    "cosmos",
    "exodus",
    "wallet"
  ],
  "bugs": {
    "url": "https://github.com/cosmology-tech/cosmos-kit/issues"
  },
  "dependencies": {
    "@cosmos-kit/exodus-extension": "^2.0.0"
  },
  "gitHead": "0f174bf8c1799158d87dfcfb97c642e51835b8d0"
}
