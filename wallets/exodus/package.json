{
  "name": "@cosmos-kit/exodus",
  "version": "2.0.1-beta.0",
  "description": "@cosmos-kit/exodus is the Exodus integration for CosmosKit",
  "author": "devs@exodus.io",
  "homepage": "https://github.com/cosmology-tech/cosmos-kit#readme",
  "license": "SEE LICENSE IN LICENSE",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "exports": {
    "./*": {
      "types": "./dist/*.d.ts",
      "import": "./dist/*.mjs",
      "require": "./dist/*.js",
      "default": "./dist/*.js"
    },
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "directories": {
    "lib": "src"
  },
  "files": [
    "dist",
    "!CHANGELOG.md"
  ],
  "scripts": {
    "build:cjs": "yarn tsc -p tsconfig.json --outDir dist --module commonjs",
    "build:mjs": "yarn tsc -p tsconfig.json --outDir mjs --module es2022 --declaration false",
    "build": "rm -rf dist && yarn build:cjs && yarn build:mjs && find mjs -type f -name *.js -exec rename -S '.js' '.mjs' {} +  && find mjs -type f -name *.mjs -exec rename -S 'mjs/' 'dist/' {} + && rm -rf mjs",
    "prepare": "npm rum build",
    "lint": "eslint --ext .tsx,.ts .",
    "format": "eslint --ext .tsx,.ts --fix .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:debug": "node --inspect node_modules/.bin/jest --runInBand"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cosmology-tech/cosmos-kit"
  },
  "keywords": [
    "cosmos-kit",
    "cosmos",
    "exodus",
    "wallet"
  ],
  "bugs": {
    "url": "https://github.com/cosmology-tech/cosmos-kit/issues"
  },
  "dependencies": {
    "@cosmos-kit/exodus-extension": "^2.0.1-beta.0"
  },
  "gitHead": "0f174bf8c1799158d87dfcfb97c642e51835b8d0"
}
