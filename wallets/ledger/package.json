{
  "name": "@cosmos-kit/ledger",
  "version": "1.0.0",
  "description": "cosmos-kit wallet connector",
  "author": "developers@cosmology.tech",
  "contributors": [
    {
      "name": "Jun Liu"
    },
    {
      "name": "Tom Wang"
    },
    {
      "name": "Dan Lynch"
    },
    {
      "name": "Noah Saso"
    },
    {
      "name": "Eliot Baker"
    },
    {
      "name": "Delivan Jeonghyeok Yoo"
    }
  ],
  "homepage": "https://github.com/cosmology-tech/cosmos-kit#readme",
  "license": "SEE LICENSE IN LICENSE",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "tsup": {
    "clean": true,
    "dts": true,
    "dts-resolve": true,
    "entry": [
      "src/**/*.ts"
    ],
    "format": [
      "cjs",
      "esm"
    ],
    "splitting": false,
    "sourcemap": true
  },
  "directories": {
    "lib": "src",
    "test": "__tests__"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "build:ts": "tsc --project ./tsconfig.json",
    "prepare": "npm run build",
    "dev": "yarn watch",
    "watch": "tsup --watch",
    "lint": "eslint --ext .tsx,.ts .",
    "format": "eslint --ext .tsx,.ts --fix .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:debug": "node --inspect node_modules/.bin/jest --runInBand"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cosmology-tech/cosmos-kit"
  },
  "keywords": [
    "cosmos",
    "cosmos-kit",
    "ledger",
    "wallet"
  ],
  "bugs": {
    "url": "https://github.com/cosmology-tech/cosmos-kit/issues"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "dist/"
    ]
  },
  "devDependencies": {
    "@babel/cli": "7.18.10",
    "@babel/core": "7.19.1",
    "@babel/eslint-parser": "7.19.1",
    "@babel/node": "7.10.5",
    "@babel/plugin-proposal-class-properties": "7.18.6",
    "@babel/plugin-proposal-export-default-from": "7.18.10",
    "@babel/plugin-proposal-object-rest-spread": "7.18.9",
    "@babel/plugin-transform-runtime": "7.19.1",
    "@babel/preset-env": "7.19.1",
    "@babel/preset-typescript": "7.16.7",
    "@babel/runtime": "7.11.2",
    "@cosmjs/cosmwasm-stargate": "0.29.0",
    "@cosmjs/stargate": "0.29.0",
    "@keplr-wallet/types": "0.11.21",
    "@types/jest": "29.0.3",
    "@typescript-eslint/eslint-plugin": "5.38.0",
    "@typescript-eslint/parser": "5.38.0",
    "babel-jest": "29.0.3",
    "cross-env": "7.0.2",
    "eslint": "8.23.1",
    "eslint-config-next": "12.3.1",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-simple-import-sort": "8.0.0",
    "eslint-plugin-unused-imports": "2.0.0",
    "jest": "29.0.3",
    "jest-in-case": "1.0.2",
    "prettier": "2.1.2",
    "regenerator-runtime": "0.13.7",
    "ts-jest": "29.0.1",
    "typescript": "4.8.3"
  },
  "dependencies": {
    "@cosmos-kit/core": "^2.0.0",
    "@ledgerhq/hw-app-cosmos": "^6.28.1",
    "@ledgerhq/hw-transport-webhid": "^6.27.15",
    "@ledgerhq/hw-transport-webusb": "^6.27.15"
  },
  "peerDependencies": {
    "@cosmjs/amino": ">= 0.28",
    "@cosmjs/proto-signing": ">= 0.28"
  },
  "gitHead": "0f174bf8c1799158d87dfcfb97c642e51835b8d0"
}
