{
  "name": "@cosmos-kit/station-extension",
  "version": "2.0.1-beta.0",
  "description": "@cosmos-kit/station-extension is the Station integration for CosmosKit",
  "author": "kanisorn@alleslab.com",
  "contributors": [
    {
      "name": "kanisorn"
    }
  ],
  "homepage": "https://github.com/cosmology-tech/cosmos-kit#readme",
  "license": "SEE LICENSE IN LICENSE",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "directories": {
    "lib": "src",
    "test": "__tests__"
  },
  "files": [
    "dist",
    "!CHANGELOG.md"
  ],
  "scripts": {
    "build:cjs": "yarn tsc -p tsconfig.json --outDir dist --module commonjs",
    "build:mjs": "yarn tsc -p tsconfig.json --outDir mjs --module es2022 --declaration false",
    "build": "rm -rf dist && yarn build:cjs && yarn build:mjs && find mjs -type f -name *.js -exec rename -S '.js' '.mjs' {} +  && find mjs -type f -name *.mjs -exec rename -S 'mjs/' 'dist/' {} + && rm -rf mjs",
    "prepare": "npm rum build",
    "lint": "eslint --ext .tsx,.ts .",
    "format": "eslint --ext .tsx,.ts --fix .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:debug": "node --inspect node_modules/.bin/jest --runInBand"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cosmology-tech/cosmos-kit"
  },
  "keywords": [
    "cosmos-kit",
    "cosmos",
    "terra",
    "terra-station",
    "wallet"
  ],
  "bugs": {
    "url": "https://github.com/cosmology-tech/cosmos-kit/issues"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "dist/"
    ]
  },
  "dependencies": {
    "@babel/runtime": "7.11.2",
    "@chain-registry/types": "0.13.1",
    "@cosmos-kit/core": "^2.0.1-beta.0",
    "@terra-money/feather.js": "^1.0.4",
    "@terra-money/wallet-types": "^3.11.2"
  },
  "peerDependencies": {
    "@cosmjs/amino": ">= 0.28",
    "@cosmjs/proto-signing": ">= 0.28"
  },
  "gitHead": "0f174bf8c1799158d87dfcfb97c642e51835b8d0"
}
